package class__;

public class StringMain {

	public static void main(String[] args) {
		String a = "apple"; //literal 생성
		String b = "apple";
		
		if (a == b) {
			System.out.println("a와 b의 참조값은 같다.");	
		} else {
			System.out.println("a와 b의 참조값은 같다.");
		}
		
		String c = new String("apple");
		String d = new String("apple");
	}
}
